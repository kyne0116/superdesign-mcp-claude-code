{
  "name": "superdesign-mcp-server",
  "version": "1.0.0",
  "description": "MCP server that wraps Superdesign AI design agent for Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "config:generate": "node scripts/generate-mcp-config.js",
    "config:generate:project": "node scripts/generate-mcp-config.js --project",
    "config:show": "node scripts/generate-mcp-config.js --show-only",
    "config:doctor": "node scripts/config-doctor.js",
    "setup": "node scripts/setup.js",
    "setup:dev": "node scripts/setup.js --project",
    "setup:quick": "node scripts/setup.js --skip-deps"
  },
  "keywords": [
    "mcp",
    "cursor",
    "superdesign",
    "tools"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "dotenv": "^17.2.1",
    "glob": "^10.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "bin": {
    "superdesign-mcp": "./dist/index.js"
  }
}
